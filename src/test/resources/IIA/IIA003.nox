// Policy for Conformance Test IIA003.
policy IIA003 denyOverrides
{
// A subject with a "bogus" attribute with a value of
// "Physician" can read or write Bart Simpson's medical
	target
	{
		permit if
		(
			subject.bogus.oneAndOnly() == "Physician" &&
			resource.id.oneAndOnly().uri() == "http://medico.com/record/patient/BartSimpson".uri() &&
			( action.id.oneAndOnly() == "read" || action.id.oneAndOnly() == "write" )
		)
	}
}