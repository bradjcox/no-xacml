// Policy for Conformance Test IIA006.
policy IIA006 denyOverrides
{
// Julius Hibbert can read or write Bart Simpson's medical record.
	target
	{
		permit if
		(
			subject.id == "Julius Hibbert" &&
			subject.someAttribute == "riddle me this" &&
			resource.id.uri() == "http://medico.com/record/patient/BartSimpson".uri() &&
			( action.id == "read" || action.id == "write" )
			// Condition
			&& "riddle me this".isIn(subject.category.accessSubject.required())
		)
	}
}